{
  "name": "express-boilerplate",
  "description": "A project scaffold for use with Express.js",
  "keywords": [
    "express",
    "boilerplate",
    "skeleton"
  ],
  "homepage": "https://github.com/kunagpal/express-boilerplate",
  "bugs": {
    "url": "https://waffle.io/kunagpal/express-boilerplate"
  },
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "check": "node ./node_modules/.bin/npm-check -u && npm shrinkwrap --dev",
    "docs": "node ./node_modules/.bin/jsdoc -c .jsdocrc bin routes scripts app.js",
    "start": "npm run pack && node ./node_modules/.bin/forever ./bin/www",
    "stop": "node ./node_modules/.bin/forever stop ./bin/www",
    "seed": "node ./scripts/database/seed.js",
    "purge": "node ./scripts/database/seed.js",
    "restart": "node ./node_modules/.bin/forever restart ./bin/www",
    "security": "node ./node_modules/.bin/nsp check",
    "lint-css": "node ./node_modules/.bin/csslint public/stylesheets",
    "lint-js": "node ./node_modules/.bin/eslint .",
    "lint": "npm run lint-js && npm run lint-css",
    "test-unit": "node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha",
    "test-structure": "node ./node_modules/.bin/mocha test/structure/* --recursive",
    "test": "npm run lint && npm run test-structure && npm run test-unit",
    "pack": "node ./scripts/misc/pack.js",
    "postinstall": "npm shrinkwrap --dev && node scripts/misc/postInstall.js"
  },
  "contributors": [
    {
      "name": "Kunal Nagpal",
      "email": "kunagpal@gmail.com"
    }
  ],
  "dependencies": {
    "async": "2.0.1",
    "body-parser": "1.15.2",
    "bower": "1.7.9",
    "clean-css": "3.4.19",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "csurf": "1.9.0",
    "debug": "2.2.0",
    "ejs": "2.5.2",
    "emailjs": "1.0.7",
    "express": "4.14.0",
    "express-session": "1.14.1",
    "express-status-monitor": "0.1.3",
    "forever": "0.15.2",
    "helmet": "2.2.0",
    "heroku-client": "3.0.0-beta3",
    "lodash": "4.15.0",
    "mongodb": "2.2.10",
    "morgan": "1.7.0",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth2": "0.1.6",
    "raven": "0.12.1",
    "uglify-js": "2.7.3"
  },
  "devDependencies": {
    "csslint": "1.0.2",
    "dotenv": "2.0.0",
    "eslint": "3.5.0",
    "eslint-plugin-jsdoc": "2.3.1",
    "eslint-plugin-lodash": "1.10.3",
    "eslint-plugin-mocha": "4.5.1",
    "eslint-plugin-mongodb": "0.2.4",
    "eslint-plugin-security": "1.2.0",
    "faker": "3.1.0",
    "ink-docstrap": "1.3.0",
    "istanbul": "0.4.5",
    "js-yaml": "3.6.1",
    "jsdoc": "3.4.1",
    "mocha": "3.0.2",
    "npm-check": "5.2.3",
    "nsp": "2.6.1",
    "supertest": "2.0.0"
  },
  "optionalDependencies": {
    "bcrypt": "0.8.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunagpal/express-boilerplate"
  },
  "license": "GPL-3.0+",
  "engines": {
    "node": "6.x"
  }
}
